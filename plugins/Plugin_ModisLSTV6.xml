	</Summary>
	<QualityControl>
		<Level>HIGHEST</Level>
		<Level>MODERATE</Level>
		<Level>LOW</Level>
		<Level>NOSCREENING</Level>
	</QualityControl>
	<ExtraInfo>
		<Tiles>true</Tiles>
	</ExtraInfo>
</PluginMetadata>
